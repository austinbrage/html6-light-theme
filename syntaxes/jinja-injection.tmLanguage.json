{
  "scopeName": "text.jinja.interpolation",
  "injectionSelector": "L:text.html -comment",
  "patterns": [
    {
      "begin": "\\{\\{",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.interpolation.begin.jinja" }
      },
      "end": "\\}\\}",
      "endCaptures": {
        "0": { "name": "punctuation.definition.interpolation.end.jinja" }
      },
      "name": "meta.embedded.expression.jinja",
      "patterns": [
        {
          "name": "variable.other.jinja",
          "match": "[^\\{\\}]+"
        }
      ]
    }
  ],
  "repository": {}
}

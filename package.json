{
  "name": "html6-light-theme",
  "displayName": "html6-light-theme",
  "description": "Light theme for HTML6 syntax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "jinja",
        "scopeName": "text.jinja.interpolation",
        "path": "./syntaxes/jinja-injection.tmLanguage.json",
        "injectTo": [
          "text.html.basic",
          "text.html.vue",
          "text.html.jinja"
        ]
      }
    ],
    "themes": [
      {
        "label": "HTML6 Light Theme",
        "uiTheme": "vs",
        "path": "./themes/html6-light-theme-color-theme.json"
      }
    ]
  }
}
